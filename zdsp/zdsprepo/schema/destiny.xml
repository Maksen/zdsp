      <property name="questid" type="int" uitext="Quest ID"/>
    </element>

    <element name="HeroMemory" useexcel="true">
      <property name="heroid" type="int" uitext="Hero ID" unique="true"/>
      <property name="name" type="localizedstring" uitext="Name"/>
      <property name="orderno" type="int" uitext="Order Number"/>
      <property name="avatarpath" type="string" uitext="Avatar Path"/>
    </element>

    <element name="HeroDialogueClue" useexcel="true">
      <property name="dialogueid" type="int" uitext="Dialogue ID" unique="true"/>
      <property name="lvl" type="int" uitext="Level"/>
      <property name="avatarpath" type="string" uitext="Avatar Path"/>
      <property name="text" type="localizedstring" uitext="Text"/>
      <property name="isuse" type="bool" uitext="Use?" default="true"/>
      <property name="probability" type="int" uitext="Probability"/>
      <property name="reward" type="int" uitext="Reward Id"/>
    </element>

    <element name="TimeClue" useexcel="true">
      <property name="dialogueid" type="int" uitext="Dialogue ID" unique="true"/>
      <property name="avatarpath" type="string" uitext="Avatar Path"/>
      <property name="category" type="ClueCategory" uitext="Category"/>
      <property name="text" type="localizedstring" uitext="Text"/>
      <property name="filepath" type="string" uitext="File Path"/>
      <property name="message" type="localizedstring" uitext="Message"/>
      <property name="task" type="localizedstring" uitext="Task"/>
      <property name="isuse" type="bool" uitext="Use?" default="true"/>
      <property name="probability" type="int" uitext="Probability"/>
      <property name="time" type="int" uitext="Time (Minute)" default="1"/>
      <property name="questid" type="int" uitext="Quest ID"/>
    </element>
  </data>
</magicalarea51>