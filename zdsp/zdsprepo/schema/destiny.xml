<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>
    <element name="DestinyClue" useexcel="true">
      <property name="clueid" type="int" uitext="Clue ID" unique="true"/>
      <property name="condition1" type="ClueCondition" uitext="Condition 1"/>
      <property name="condition1id" type="int" uitext="Condition 1 ID"/>
      <property name="condition2" type="ClueCondition" uitext="Condition 2"/>
      <property name="condition2id" type="int" uitext="Condition 2 ID"/>
      <property name="condition3" type="ClueCondition" uitext="Condition 3"/>
      <property name="condition3id" type="int" uitext="Condition 3 ID"/>
      <property name="avatarpath" type="string" uitext="Avatar Path"/>
      <property name="category" type="ClueCategory" uitext="Category"/>
      <property name="text" type="localizedstring" uitext="Text"/>
      <property name="filepath" type="string" uitext="File Path"/>
      <property name="message" type="localizedstring" uitext="Message"/>
      <property name="task" type="localizedstring" uitext="Task"/>
      <property name="memorytype" type="ClueMemoryType" uitext="Memory Category"/>
