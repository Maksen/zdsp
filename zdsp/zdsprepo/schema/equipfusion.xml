<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>

    <element name="FusionPartsList" useexcel="true">
      <property name="typename" type="string" uitext="Type Name"/>
      <property name="type_id" type="int" uitext="Type Id"/>
      <property name="sort_id" type="int" uitext="Sort Id"/>
    </element>
    
    <element name="EquipFusionSideEffect" useexcel="true">
      <property name="remark" type="string" uitext="Remark" noexport="true"/>
      <property name="group_id" type="string" uitext="Group Id"/>
      <property name="sideeffectid" type="string" uitext="SideEffectId"/>
      <property name="se_rarity" type="int" uitext="Se Rarity" default="0"/>
      <property name="weight" type="int" uitext="Weight" default="0"/>
    </element>

    <element name="EquipFusionCost" useexcel="true">
      <property name="stone_rarity" type="int" uitext="Stone Rarity"/>
      <property name="cost_currency" type="int" uitext="Cost Currency"/>
    </element>

    <element name="EquipFusionAdditionRule" useexcel="true">
      <property name="se_rarity" type="int" uitext="Se Rarity"/>
      <property name="weight" type="int" uitext="Weight"/>
    </element>

    <element name="EquipFusionSell" useexcel="true">
      <property name="sell_rarity" type="int" uitext="Sell Rarity"/>
      <property name="token_id" type="int" uitext="Token Id" default="0"/>
      <property name="token_amout" type="int" uitext="Token Amout" default="0"/>
    </element>

    <element name="EquipFusionStore" useexcel="true">
      <property name="stone_id" type="int" uitext="Stone Id"/>
      <property name="token_id" type="int" uitext="Token Id" default="0"/>
      <property name="token_amout" type="int" uitext="Token Amout" default="0"/>
    </element>

  </data>
</magicalarea51>