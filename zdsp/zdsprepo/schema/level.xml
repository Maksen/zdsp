<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>
    <enum name="RealmType">
      <item key="RealmWorld" value="0"/>
      <item key="DungeonStory" value="1"/>
      <item key="DungeonDailySpecial" value="2"/>
      <!--<item key="RealmTutorial" value="3"/>
      <item key="ActivityGuardWar" value="4"/>
      <item key="ActivityGuildSMBoss" value="5"/>
      <item key="ActivityWorldBoss" value="6"/>
      <item key="Arena" value="7"/>
      <item key="InvitePVP" value="8"/>
      <item key="EliteMap" value="9"/>-->
    </enum>

    <enum name="MapType">
      <item key="City" value="0"/>
      <item key="Wilderness" value="1"/>
      <item key="Dungeon" value="2"/>
      <item key="Activity" value="3"/>
      <item key="Others" value="4"/>
    </enum>

    <enum name="DungeonDifficulty">
      <item key="Easy" value="0"/>
      <item key="Hard" value="1"/>
      <item key="Hell" value="2"/>
    </enum>

    <enum name="LevelPVPType">
      <item key="Peace" value="0"/>
      <item key="FreeForAll" value="2"/>
      <item key="Guild" value="4"/>
      <item key="Faction" value="5"/>
    </enum>

    <enum name="DungeonType">
      <item key="Daily" value="0"/>
      <item key="Special" value="1"/>
    </enum>

    <enum name="RealmObjectiveType">
      <item key="TimeWithinNoDeath" value="0"/>
      <item key="TimeWithinComplete" value="1"/>
      <item key="HeroUseOnly" value="2"/>
      <item key="HeroCannotUse" value="3"/>
      <item key="HeroTypeCannotUse" value="4"/>
      <item key="SkillCannotUseAny" value="5"/>
      <item key="HeroQualityUseOnly" value="6"/>
      <item key="NPCKills" value="7"/>
      <item key="RealmComplete" value="8"/>
    </enum>

    <enum name="ForbiddenUsesType">
      <item key="Item" value="0"/>
      <item key="SideEffect" value="1"/>
      <item key="Skill" value="2"/>
    </enum>

    <element name="MapCategory" useexcel="true" collectionkey="categoryname">
      <property name="categoryname" type="string" uitext="Name"/>
      <property name="maptype" type="MapType" uitext="Map Type"/>
      <property name="localizedname" type="localizedstring" uitext="Name(Localized)"/>
    </element>
  
    <element name="Level" useexcel="true" collectionkey="excelname">
      <property name="excelname" type="string" uitext="Name"/>
      <property name="localizedname" type="localizedstring" uitext="Name(Localized)"/>
      <property name="unityscene" type="string" uitext="Unity Scene" unique="true"/>
      <property name="maptexture" type="asset" uitext="Big Map Texture" assettype="sprite"/>
      <property name="banitemtype" type="string" uitext="Ban Item Type" default="" info="Seperated by ;"/>
      <property name="banitemid" type="string" uitext="Ban Item Id" default="" info="Seperated by ;"/>
      <property name="effectpath" type="asset" uitext="Scene Effect Prefab Path" default="" assettype="prefab"/>
    </element>

    <element name="Realm" useexcel="true" collectionkey="excelname">
      <property name="excelname" type="string" uitext="Name"/>
      <property name="localizedname" type="localizedstring" uitext="Name(Localized)"/>
      <property name="level" type="Level" uitext="Level" display="unityscene"/>
      <constant name="type" type="RealmType" value="RealmType.RealmWorld"/>
      <property name="reqlvl" type="int" uitext="Required Level" default="1"/>
      <property name="pvptype" type="LevelPVPType" uitext="PVP Type" default="0"/>
      <property name="preparation" type="int" uitext="Preparation Time" default="0" info="0 means no preparation time"/>
      <property name="timelimit" type="int" uitext="Time Limit" default="600" info="0 means no limit"/>
      <property name="respawntype" type="RespawnType" uitext="Respawn Type"/>
      <property name="respawncd" type="int" uitext="Respawn Countdown" default="30" info="Seconds"/>
      <property name="minplayer" type="int" uitext="Min Player" default="1" info="0 means no min player"/>
      <property name="maxplayer" type="int" uitext="Max Player" default="0" info="0 means no limit"/>
      <property name="maptype" type="MapType" uitext="Map Type"/>
    </element>

    <element name="RealmWorld" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.RealmWorld"/>
      <property name="worldseq" type="int" uitext="World Sequence" default="1"/>
    </element>

    <element name="DungeonStory" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.DungeonStory"/>
      <property name="sequence" type="int" uitext="Story Sequence" default="1"/>
      <property name="iconpath" type="asset" uitext="UI Icon Path" default="" assettype="sprite"/>
      <property name="difficulty" type="DungeonDifficulty" uitext="Difficulty"/>
      <property name="dailyentry" type="int" uitext="Daily Entry" default="3"/>
      <property name="displaygrp" type="RewardGroup" uitext="Display Group" display="name"/>
      <property name="rewardgrp" type="RewardGroup" uitext="Reward Group" display="name"/>
    </element>
    
    <element name="DungeonDailySpecial" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.DungeonDailySpecial"/>
      <property name="dungeontype" type="DungeonType" uitext="Dungeon Type" default="0"/>
      <property name="sequence" type="int" uitext="Sequence" default="1"/>
      <property name="iconpath" type="asset" uitext="UI Icon Path" default="" assettype="sprite"/>
      <property name="dailyentry" type="int" uitext="Daily Entry" default="3"/>
      <property name="displaygrp" type="RewardGroup" uitext="Display Group" display="name"/>
      <property name="rewardgrp" type="RewardGroup" uitext="Reward Group" display="name"/>
      <property name="isopenday1" type="bool" uitext="Is Open Day 1" default="false"/>
      <property name="isopenday2" type="bool" uitext="Is Open Day 2" default="false"/>
      <property name="isopenday3" type="bool" uitext="Is Open Day 3" default="false"/>
      <property name="isopenday4" type="bool" uitext="Is Open Day 4" default="false"/>
      <property name="isopenday5" type="bool" uitext="Is Open Day 5" default="false"/>
      <property name="isopenday6" type="bool" uitext="Is Open Day 6" default="false"/>
      <property name="isopenday7" type="bool" uitext="Is Open Day 7" default="false"/>
    </element>

    <element name="RealmObjective" useexcel="true" collectionkey="excelname">
      <property name="excelname" type="string" uitext="Name"/>
      <property name="localizedname" type="localizedstring" uitext="Name(Localized)"/>
      <property name="localizeddescription" type="localizedstring" uitext="Localized Description" display="name"/>
      <property name="realmobjectivetype" type="RealmObjectiveType" uitext="Realm Objective Type" default="0"/>
      <property name="timelimit" type="int" uitext="Time Limit" default="600" info="Time in seconds"/>
      <!--<property name="hero" type="HeroCard" uitext="Hero" display="name" info="For Hero objective"/>
      <property name="herotype" type="HeroType" uitext="Hero Type" default="0" info="For HeroType objective"/>
      <property name="heroquality" type="HeroQuality" uitext="Hero Quality" default="0" info="For HeroQuality objective"/>-->
      <property name="monsterid" type="CombatNPC" uitext="NPC Monster" display="archetype" info="For NPCKills objective"/>
      <property name="monsterkillcnt" type="int" uitext="NPC Monster KillCount" default="0" info="For NPCKills objective"/>
    </element>

    <!--<element name="ExtraEntryFees" useexcel="true" collectionkey="excelname">
      <property name="excelname" type="string" uitext="Name"/>
      <property name="sequence" type="int" uitext="Story Sequence" default="1"/>
      <property name="count" type="int" uitext="Count" default="1"/>
      <property name="gold" type="int" uitext="gold" default="0"/>
    </element>
    
    <element name="StarReward" useexcel="true" collectionkey="excelname">
      <property name="excelname" type="string" uitext="Name"/>
      <property name="sequence" type="int" uitext="Story Sequence" default="1"/>
      <property name="starnumber" type="int" uitext="Star Number" default="0" info="Amount to get star reward"/>
      <property name="rewardgrp" type="RewardGroup" uitext="Reward Group" display="name"/>
    </element>

    <element name="ActivityGuardWar" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.ActivityGuardWar"/>
      <property name="heroid" type="int" uitext="HeroId"/>
      <property name="boss" type="CombatNPC" uitext="Boss Archetype" display="archetype"/>
      <property name="worldlv" type="int" uitext="World Lv"/>
      <property name="tower" type="CombatNPC" uitext="Tower Archetype" display="archetype"/>
    </element>
    
    <element name="ActivityGuildSMBoss" inherit="Realm" useexcel="true">
      <constant name="type" type="RealmType" value="RealmType.ActivityGuildSMBoss"/>
      <property name="localizeddescription" type="localizedstring" uitext="Localized Description" display="name"/>
    </element>  
    
    <element name="RealmTutorial" inherit="Realm" useexcel="true">
      <constant name="type" type="RealmType" value="RealmType.RealmTutorial"/>
      <property name="localizeddescription" type="localizedstring" uitext="Localized Description" display="name"/>
    </element>

    <element name="ActivityWorldBoss" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.ActivityWorldBoss"/>
      <property name="requirelvlmin" type="int" uitext="Require Level Min" default="1"/>
      <property name="boss" type="CombatNPC" uitext="Boss Archetype" display="archetype"/>
      <property name="dailyentry" type="int" uitext="Daily Entry" default="1"/>
      <property name="opendaily" type="string" uitext="Open Daily" default="3:06:00|7:09:00" info="week:hour:minute|week:hour:minute"/>
      <property name="reward1" type="RewardList" uitext="Reward 1" display="name"/>
      <property name="reward2" type="RewardList" uitext="Reward 2" display="name"/>
      <property name="reward3" type="RewardList" uitext="Reward 3" display="name"/>
