      <property name="battlecd" type="int" uitext="Battle CD" default="50" info="seconds"/>
    </element>

    <element name="ArenaFakeRank" useexcel="true" collectionkey="rank" localizeexport="rank">
      <property name="rank" type="int" uitext="Arena Rank" unique="true"/>
      <property name="localizedname" type="localizedstring" uitext="LocalizedName"/>
      <property name="jobsect" type="int" uitext="Jobsect"/>
      <property name="level" type="int" uitext="Level"/>
      <property name="equipscore" type="int" uitext="Equip Score"/>
      <property name="equipupgrade" type="int" uitext="Equipment Upgrade" default="0"/>
      <property name="outfitid" type="int" uitext="Outfit Asset" default="1"/>
      <property name="weaponid" type="int" uitext="Weapon Asset" default="0"/>
      <property name="heroquality" type="HeroQuality" uitext="Hero Quality"/>
      <property name="cardlvl" type="int" uitext="Skill Level" default="1"/>
    </element>

    <element name="ActivityNotification" useexcel="true" collectionkey="name">
      <property name="name" type="string" uitext="Name"/>
      <property name="localizedname" type="localizedstring" uitext="Content(Localized)"/>
      <property name="type" type="NotificationType" uitext="Notification Type"/>
      <property name="priority" type="int" uitext="Priority"/>
      <property name="duration" type="int" uitext="Minutes" default="2"/>
      <property name="requirelvlmin" type="int" uitext="Require Level Min"/>
    </element>

    <element name="InvitePVP" inherit="Realm" useexcel="true">
      <constant name="type" type="RealmType" value="RealmType.InvitePVP"/>
      <property name="localizeddescription" type="localizedstring" uitext="Localized Description" display="name"/>
    </element>

    <element name="EliteMap" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.EliteMap"/>
      <property name="dailytime" type="int" uitext="Daily Time(sec)"/>
    </element>-->
    
  </data>
</magicalarea51>
