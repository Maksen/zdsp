    <element name="Arena" inherit="Realm" useexcel="true" collectionkey="excelname">
      <constant name="type" type="RealmType" value="RealmType.Arena"/>
      <property name="requirelvlmin" type="int" uitext="Require Level Min" default="155"/>
      <property name="dailyentry" type="int" uitext="Daily Entry" default="10"/>
      <property name="rewardcd" type="int" uitext="Reward CD" default="22" info="hours"/>
      <property name="reward1" type="RewardList" uitext="Reward 1" display="name"/>
      <property name="reward2" type="RewardList" uitext="Reward 2-3" display="name"/>
      <property name="reward3" type="RewardList" uitext="Reward 4-10" display="name"/>
      <property name="reward4" type="RewardList" uitext="Reward 11-50" display="name"/>
      <property name="reward5" type="RewardList" uitext="Reward 51-100" display="name"/>
