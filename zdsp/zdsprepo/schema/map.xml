<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>
    
	<element name="WorldMapCountry" useexcel="true">
		<property name="countryname" type="string" uitext="localizedname"/>
		<property name="lv" type="string" uitext="level range"/>
	</element>
	
	<element name="WorldMapCountryPlace" useexcel="true">
		<property name="country" type="WorldMapCountry" uitext="world map country" display="countryname"/>
		<property name="level" type="Level" uitext="level" display="excelname"/>
		<property name="placeiconpath" type="string" uitext="place icon asset path" default=""/>
		<property name="placename" type="string" uitext="Place or Interest Name" default=""/>
		<property name="posx" type="float" uitext="Map marker position X" default="0.0"/>
		<property name="posy" type="float" uitext="Map marker position Y" default="0.0"/>
		<property name="posz" type="float" uitext="Map marker position Z" default="0.0"/>
	</element>
	
	<element name="WorldMapCountryMonster" useexcel="true">
		<property name="country" type="WorldMapCountry" uitext="world map country" display="countryname"/>
		<property name="archetype" type="CombatNPC" uitext="Monster Archetype" display="archetype"/>
	</element>
    
  </data>
</magicalarea51>