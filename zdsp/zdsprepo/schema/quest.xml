<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>   
    <element name="Chapter" useexcel="true">
      <property name="groupid" type="int" uitext="Group ID"/>
      <property name="name" type="localizedstring" uitext="Group Name"/>
      <property name="description" type="localizedstring" uitext="Group Description"/>
      <property name="questid" type="int" uitext="Quest ID"/>
      <property name="sequence" type="int" uitext="Quest Sequence"/>
      <property name="icon" type="string" uitext="Chapter Long Background"/>
      <property name="background" type="string" uitext="Chapter Background"/>
    </element>

    <element name="Wonderful" useexcel="true">
      <property name="name" type="localizedstring" uitext="Movie Name"/>
      <property name="description" type="localizedstring" uitext="Movie Description"/>
      <property name="path" type="string" uitext="Movie Path"/>
      <property name="background" type="string" uitext="Movie Background"/>
      <property name="questid" type="string" uitext="Quest Id"/>
    </element>
      
    <element name="Quest" useexcel="true" collectionkey="questid">
      <property name="questid" type="int" uitext="Quest ID" unique="true"/>
      <property name="questname" type="localizedstring" uitext="Quest Name(Localized)"/>
      <property name="subname" type="localizedstring" uitext="SubName(Localized)"/>
      <property name="description" type="localizedstring" uitext="Description"/>
      <property name="starttalkid" type="int" uitext="Start Talk Id"/>
      <property name="type" type="QuestType" uitext="Quest Type"/>
      <property name="promptaccept" type="bool" uitext="Prompt Accept"/>
      <property name="promptobj" type="bool" uitext="Prompt Objective"/>
      <property name="minlv" type="int" uitext="Quest Level"/>
      <property name="requirementid" type="int" uitext="Requirement Group Id"/>
      <property name="frontquest" type="int" uitext="Front Quest" default="-1"/>
      <property name="nextquest" type="string" uitext="Next Quest"/>
      <property name="isopen" type="bool" uitext="Open" default="true"/>
      <property name="repeat" type="QuestRepeatType" uitext="Repeat" default="0"/>
      <property name="triggertype" type="QuestTriggerType" uitext="Trigger Type"/>
      <property name="triggercaller" type="int" uitext="Trigger Caller"/>
      <property name="replyid" type="bool" uitext="Reply By UI"/>
      <property name="objectiveid" type="string" uitext="Objective Id"/>
      <property name="objgroup" type="string" uitext="Objective Group"/>
      <property name="eventid" type="int" uitext="Start And End Quest Event Group Id"/>
      <property name="reward" type="string" uitext="Reward"/>
      <property name="unlock" type="FeatureList" uitext="Unlock Feature" display ="name"/>
      <property name="unlockval" type="string" uitext="Unlock Value"/>
      <property name="showae" type="bool" uitext="Is Show AE"/>
      <property name="teleport" type="bool" uitext="Teleport"/>
    </element>

    <element name="QuestObjective" useexcel="true" collectionkey="objid">
      <property name="objid" type="int" uitext="Objective ID" unique="true"/>
      <property name="name" type="string" uitext="Name"/>
      <property name="localizedname" type="localizedstring" uitext="Localized Name"/>
      <property name="description" type="localizedstring" uitext="Description"/>
      <property name="time" type="int" uitext="Complete Time"/>
      <property name="type" type="QuestObjectiveType" uitext="Type"/>
      <property name="para1" type="int" uitext="Param1" default="0" info="怪物id, NPCid, 副本id, 道具id, 返回目的id"/>
      <property name="para2" type="int" uitext="Param2" default="0" info="數量, 對話id, 次數, 互動id, 下目的id"/>
      <property name="para3" type="int" uitext="Param3" default="0" info="成功率, 最後目的id" />
      <property name="para4" type="string" uitext="Param4" default="0" info="目的id" />
      <property name="eventid" type="int" uitext="Complete Objective Event Group Id"/>
      <property name="requirementid" type="int" uitext="Requirement Group Id"/>
    </element>
    
    <element name="QuestEventDetail" useexcel="true">
      <property name="groupid" type="int" uitext="Group Id"/>
      <property name="timing" type="EventTimingType" uitext="Event Timing"/>
      <property name="type" type="QuestEventType" uitext="Event Type"/>
      <property name="para1" type="string" uitext="param1"/>
      <property name="para2" type="string" uitext="param2"/>
      <property name="questid" type="int" uitext="Quest Id"/>
      <property name="objectiveid" type="int" uitext="Objective Id"/>
    </element>

    <element name="QuestTalkDetail" useexcel="true" collectionkey="name">
      <property name="talkid" type="int" uitext="Talk ID" unique="true"/>
      <property name="name" type="string" uitext="Name"/>
      <property name="steps" type="int" uitext="Total Steps" default="1" min="1" max="7"/>
      <property name="caller1" type="int" uitext="Caller 1"/>
      <property name="dialogue1" type="localizedstring" uitext="Dialogue 1"/>
      <property name="caller2" type="int" uitext="Caller 2"/>
      <property name="dialogue2" type="localizedstring" uitext="Dialogue 2"/>
      <property name="caller3" type="int" uitext="Caller 3"/>
      <property name="dialogue3" type="localizedstring" uitext="Dialogue 3"/>
      <property name="caller4" type="int" uitext="Caller 4"/>
      <property name="dialogue4" type="localizedstring" uitext="Dialogue 4"/>
      <property name="caller5" type="int" uitext="Caller 5"/>
      <property name="dialogue5" type="localizedstring" uitext="Dialogue 5"/>
      <property name="caller6" type="int" uitext="Caller 6"/>
      <property name="dialogue6" type="localizedstring" uitext="Dialogue 6"/>
      <property name="caller7" type="int" uitext="Caller 7"/>
      <property name="dialogue7" type="localizedstring" uitext="Dialogue 7"/>
      <property name="selectionid" type="int" uitext="Selection Group Id"/>
    </element>

    <element name="QuestSelectDetail" useexcel="true">
      <property name="groupid" type="int" uitext="Group ID"/>
      <property name="answer" type="localizedstring" uitext="Answer"/>
      <property name="questtalkid" type="QuestTalkDetail" uitext="Next Talk ID" display="name"/>
      <property name="isanswer" type="bool" uitext="Is Correct Answer"/>
      <property name="actiontype" type="QuestSelectionActionType" uitext="Action Type"/>
      <property name="actionid" type="int" uitext="Action ID" info="職業id, objective組id"/>
    </element>

    <element name="QuestInteractiveDetail" useexcel="true" collectionkey="interactiveid">
      <property name="interactiveid" type="int" uitext="Interactive ID" unique="true"/>
      <property name="interactivetime" type="int" uitext="Interactive Time" default="0"/>
      <property name="successrate" type="int" uitext="Success Rate" default="0"/>
      <property name="type" type="QuestInteractiveType" uitext="Interactive Type"/>
      <property name="para" type="int" uitext="Param" default="0" info="任務id"/>
      <property name="iconid" type="string" uitext="Icon Id"/>
      <property name="icontext" type="localizedstring" uitext="Icon Text"/>
      <property name="emotionid" type="string" uitext="Emotion Id"/>
      <property name="successmsg" type="localizedstring" uitext="Success Msg"/>
      <property name="failedmsg" type="localizedstring" uitext="Failed Msg"/>
    </element>

    <element name="QuestRequirementDetail" useexcel="true" collectionkey="requirementid">
      <property name="requirementid" type="int" uitext="Requirement ID" unique="true"/>
      <property name="groupid" type="int" uitext="Group ID"/>
      <property name="type" type="QuestRequirementType" uitext="Type"/>
      <property name="para1" type="int" uitext="Param1" info="等級, 道具id, 傭兵id, 稱號id, sideeffectid, NPCid"/>
      <property name="para2" type="int" uitext="Param2" info="數量, 裝備類型, 狀況"/>
      <property name="para3" type="int" uitext="Param3" info="收回?"/>
    </element>
  
    <element name="QuestDestiny" useexcel="true" collectionkey="destinyid">
      <property name="destinyid" type="int" uitext="Destiny ID" unique="true"/>
      <property name="groupid" type="int" uitext="Group ID"/>
      <property name="type" type="QuestDestinyType" uitext="Country Type"/>
      <property name="sequence" type="int" uitext="Sequence"/>
      <property name="questid" type="int" uitext="Quest ID"/>
      <property name="name" type="localizedstring" uitext="Name"/>
      <property name="path" type="string" uitext="path"/>
      <property name="posx" type="int" uitext="X Position"/>
      <property name="posy" type="int" uitext="Y Position"/>
      <property name="site" type="localizedstring" uitext="Site"/>
      <property name="uicolumn" type="int" uitext="Column"/>
      <property name="uirow" type="int" uitext="Row"/>
      <property name="next" type="string" uitext="Next Block"/>
    </element>
  </data>
</magicalarea51>