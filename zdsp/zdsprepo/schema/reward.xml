<?xml version="1.0" encoding="utf-8"?>
<magicalarea51>
  <data>
    
	  <element name="RewardList" useexcel="true">
		  <property name="name"          type="string" uitext="Name" noexport="true"/>
		  <property name="rewardgroupid" type="int"    uitext="Reward Group ID"/>
		  <property name="isexpgrade"    type="bool"   uitext="Is Exp Grade"        default="false" info="false = no scaling, true = job and level scaling "/>
		  <property name="job"           type="int"    uitext="Occupation / Job"    default="-1"    info="Refer to JobType, -1 for no job"/>
		  <property name="experience"    type="int"    uitext="Experience"          default="0"/>
		  <property name="jobexperience" type="int"    uitext="Job Experience"      default="0"/>
		  <property name="money"         type="int"    uitext="Money"               default="0"/>
		  <property name="donatept"      type="int"    uitext="Donate Points"       default="0"/>
		  <property name="guildactivept" type="int"    uitext="Guild Active Points" default="0"/>
		  <property name="itemid1"       type="int"    uitext="item id 1"           default="0"/>
		  <property name="itemcount1"    type="int"    uitext="item 1 count"        default="0"/>
		  <property name="itemid2"       type="int"    uitext="item id 2"           default="0"/>
		  <property name="itemcount2"    type="int"    uitext="item 2 count"        default="0"/>
		  <property name="itemid3"       type="int"    uitext="item id 3"           default="0"/>
		  <property name="itemcount3"    type="int"    uitext="item 3 count"        default="0"/>
		  <property name="itemid4"       type="int"    uitext="item id 4"           default="0"/>
		  <property name="itemcount4"    type="int"    uitext="item 4 count"        default="0"/>
		  <property name="itemid5"       type="int"    uitext="item id 5"           default="0"/>
		  <property name="itemcount5"    type="int"    uitext="item 5 count"        default="0"/>
	  </element>
	
	  <element name="ExperienceRate" useexcel="true">
		  <property name="level"    type="int"   uitext="Level"/>
		  <property name="exprate"  type="float" uitext="Experience Rate"/>
		  <property name="jexprate" type="float" uitext="Job Experience Rate"/>
	  </element>
	
	  <element name="ActivityReward" useexcel="true">
		  <property name="activitygroupid" type="int"    uitext="Activity Group ID"/>
		  <property name="score"           type="int"    uitext="Score"/>
		  <property name="rewardgroupid"   type="string" uitext="Reward Group ID"/>
	  </element>
    
  </data>
</magicalarea51>